<h2>No of Events (Yearly)</h2>
<table>
    <tr>
        <th>Event Year</th>
        <th>No of Events</th>
    </tr>
    {% for key, value in years %}
        <tr>
            <td>{{key}}</td>
            <td>{{value}}</td>
        </tr>
    {% endfor %}
</table>

<h2>No of Events (Quarterwise)</h2>
<table>
    <tr>
        <th>Event Year</th>
        <th>Quarter</th>
        <th>No of Events</th>
    </tr>
    {% for year, quarters in quarters %}
        {% for quarter, count in quarters %}
            {% if count != 0 %}
                <tr>
                    <td>{{ year }}</td>
                    <td>{{ quarter }}</td>
                    <td>{{ count }}</td>
                </tr>
            {% endif %}
        {% endfor %}
    {% endfor %}
</table>

<h2>No of Event Types</h2>
<table>
    <tr>
        <th>Event Type</th>
        <th>No Of Events</th>
    </tr>
    {% for key, value in event_types %}
        <tr>
            <td>{{key}}</td>
            <td>{{value}}</td>
        </tr>
    {% endfor %}
</table>
